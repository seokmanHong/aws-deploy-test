{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "Advertiser-api-container",
            "image": "323538435273.dkr.ecr.ap-northeast-2.amazonaws.com/advertiser-api-app:latest",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "awseb-logs-nginx-proxy",
                    "containerPath": "/web/app"
                }
            ]
        }
    ]
}
